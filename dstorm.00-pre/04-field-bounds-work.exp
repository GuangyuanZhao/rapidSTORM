begin_test_suite

set test "Field bounds work"
send "in Car in Input in MultiChannel in ChannelCount in value set 0\n"
expect {
    -re "in Car in Input in MultiChannel in ChannelCount in value set 0\r*\n" { fail "$test"; }
    -re "in Car in Input in MultiChannel in ChannelCount in value set 2\r*\n" { pass "$test"; }
}

end_test_suite
